{
  "name": "pcache",
  "version": "0.1.0",
  "description": "A files/directories caching command line tool",
  "main": "index.js",
  "bin": {
    "tagit": "./bin/pcache"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha dist/test/ --recursive",
    "test-debug": "./node_modules/.bin/mocha --debug-brk dist/test/ --recursive",
    "prepublish": "cd src && ../node_modules/.bin/tsc",
    "pretest": "cd src && ../node_modules/.bin/tsc",
    "watch": "cd src && ../node_modules/.bin/tsc -w"
  },
  "files": ["dist", "bin", "src"],
  "keywords": [
    "directory",
    "cache",
    "cli",
    "command"
  ],
  "author": {
    "name": "Matias Surdi",
    "email": "matias@surdi.net"
  },
  "license": "BSD",
  "repository": {
    "type": "git",
    "url": "https://github.com/msurdi/pcache"
  },
  "bugs": {
    "url": "https://github.com/msurdi/pcache/issues"
  },
  "dependencies": {
    "commander": "^2.8.1",
    "fs-extra": "^0.18.3",
    "parse-duration": "^0.1.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "chai": "^2.3.0",
    "mocha": "^2.2.5",
    "ncp": "^2.0.0",
    "source-map-support": "^0.2.10",
    "tmp": "0.0.26",
    "tsd": "^0.6.0",
    "typescript": "^1.5.0-alpha"
  }
}
